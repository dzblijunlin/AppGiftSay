<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_cate_strategy_collumn_all_info"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.naruto.a_presentsay.activity.CateStrategyCollumnAllInfoActivity"
    android:background="#FFF">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="#FFF"
        android:orientation="horizontal">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerInParent="true"
                android:layout_marginLeft="10dp"
                android:src="@mipmap/credit_btn_back" />

            <TextView
                android:id="@+id/class_gride_title_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="栏目"
                android:textColor="#f8201703"
                android:textSize="20sp" />

        </RelativeLayout>


    </LinearLayout>



    <android.support.v7.widget.RecyclerView
        android:id="@+id/startegy_search_all_rv"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="10"
        />

</LinearLayout>
